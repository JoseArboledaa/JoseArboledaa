        private void button1_Click(object sender, EventArgs e)
        {
            double numero1 = 0;
            double numero2 = 0;
            double result = 0;
            numero1 = Convert.ToDouble(num1.Text);
            numero2 = Convert.ToDouble(num2.Text);
            result = numero1 + numero2;
            resultado.Text = Convert.ToString(result);
        }
